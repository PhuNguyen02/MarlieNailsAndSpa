{
  "name": "marlie-nails-and-spa",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo for Marlie Nails and Spa project",
  "workspaces": [
    "backEnd",
    "frontEnd"
  ],
  "scripts": {
    "install:all": "npm install",
    "backend:dev": "npm run start:dev --workspace=backEnd",
    "backend:build": "npm run build --workspace=backEnd",
    "backend:start": "npm run start:prod --workspace=backEnd",
    "frontend:dev": "npm run dev --workspace=frontEnd",
    "frontend:build": "npm run build --workspace=frontEnd",
    "build": "npm run backend:build && npm run frontend:build",
    "start": "npm run backend:start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
